{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "dialog:default",
    "fs:default",
    "fs:allow-mkdir",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-appdata-write-recursive",
    "fs:allow-appdata-read-recursive"
  ]
}
